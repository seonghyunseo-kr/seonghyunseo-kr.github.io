[
{% for project in site.projects %}
  {
    "title": {{ project.name | jsonify }},
    "url": {{ project.url | relative_url | jsonify }},
    "year": {{ project.year | jsonify }},
    "track": {{ project.track | jsonify }},
    "topic": {{ project.topic | jsonify }},
    "description": {{ project.description | default: "" | strip_html | strip_newlines | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]